{
  "name": "Hoff Block",
  "manifest_version": 2,
  "version": "0.1",
  "description": "Replaces ads with pictures of The Hoff (or another user-defined image).",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "storage"
  ],
  "background": {
    "service_worker": "scripts/adblock.js"
  },
  "web_accessible_resources": [
    "data/domains.txt"
  ]
}